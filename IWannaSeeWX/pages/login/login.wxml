<!--pages/login/login.wxml-->
 <view class="login-masker">
    <view class='header'>
        <image src='/images/wx-login.png'></image>
    </view>
 
    <view class='content'>
        <view>申请获取以下权限</view>
        <text>获得你的公开信息(昵称，头像等)</text>
    </view>
 
    <button class='bottom' type='primary' open-type="getUserInfo" lang="zh_CN" bindgetuserinfo="loginUtil">
        授权登录
    </button>
</view>
